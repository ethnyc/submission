{
  "name": "submission",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:web": "yarn workspace web dev",
    "build": "run-s build:contracts build:web",
    "build:web": "yarn workspace web build",
    "build:contracts": "yarn workspace contracts build"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.--write": "prettier --write"
  }
}
